<view class="wuss-class wuss-date-picker" hover-class="none" hover-stop-propagation="false">
  <w-cell-picker
    isDatePicker
    label="{{ label }}"
    options="{{ options }}"
    title="{{ title }}"
    currentValue="{{ currentValue }}"
    bind:onChange="handleOnChange"
    bind:onSelect="handleSelect"
  />
</view>